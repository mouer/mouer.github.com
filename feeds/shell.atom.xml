&lt;div class="highlight"&gt;&lt;pre&gt;&lt;span class="c"&gt;# 查看http链接状态&lt;/span&gt;
netstat -n | awk &lt;span class="s1"&gt;&amp;#39;/^tcp/ {++S[$NF]} END {for(a in S) print a, S[a]}&amp;#39;&lt;/span&gt;

&lt;span class="c"&gt;# 查看80使用情况&lt;/span&gt;
netstat -an -t | grep &lt;span class="s1"&gt;&amp;#39;:80&amp;#39;&lt;/span&gt; | grep ESTABLISHED | awk &lt;span class="s1"&gt;&amp;#39;{printf &amp;quot;%s %s\n&amp;quot;, $5, $6}&amp;#39;&lt;/span&gt; | uniq | sort

&lt;span class="c"&gt;# cpu数量&lt;/span&gt;
cat /proc/cpuinfo | grep -c processor
&lt;/pre&gt;&lt;/div&gt;</summary><category term="shell"></category></entry></feed>